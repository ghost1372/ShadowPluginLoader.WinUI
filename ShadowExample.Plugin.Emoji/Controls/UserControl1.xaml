<?xml version="1.0" encoding="utf-8" ?>
<UserControl
    x:Class="ShadowExample.Plugin.Emoji.Controls.UserControl1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ce="using:CustomExtensions.WinUI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ex="using:ShadowExample.Plugin.Emoji.Extensions"
    xmlns:local="using:ShadowExample.Plugin.Emoji.Controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid>
        <!--  Use Assets File: https://plugin-loader.kituin.fun/zh/advance/assets  -->
        <Image Source="{ce:PluginImage Source=/Assets/th.jpg}" />

        <!--  Use I18N: https://plugin-loader.kituin.fun/zh/advance/i18n  -->
        <Button Content="{ex:Locale Key=TestName}" />
    </Grid>
</UserControl>
